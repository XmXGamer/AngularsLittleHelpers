{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ngx-policy-authorization": {
      "projectType": "library",
      "root": "projects/ngx-policy-authorization",
      "sourceRoot": "projects/ngx-policy-authorization/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/ngx-policy-authorization/tsconfig.lib.json",
            "project": "projects/ngx-policy-authorization/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ngx-policy-authorization/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/ngx-policy-authorization/tsconfig.spec.json"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/ngx-policy-authorization/tsconfig.lib.json",
              "projects/ngx-policy-authorization/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "ngx-feature-flags-testing": {
      "projectType": "library",
      "root": "projects/ngx-feature-flags-testing",
      "sourceRoot": "projects/ngx-feature-flags-testing/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/ngx-feature-flags-testing/tsconfig.lib.json",
            "project": "projects/ngx-feature-flags-testing/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ngx-feature-flags-testing/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/ngx-feature-flags-testing/tsconfig.spec.json"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/ngx-feature-flags-testing/tsconfig.lib.json",
              "projects/ngx-feature-flags-testing/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "ngx-policy-authorization"
}
